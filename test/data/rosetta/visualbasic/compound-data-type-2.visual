Structure Immutable_Point
   Private m_X As Integer
   Private m_Y As Integer

   Public Sub New(ByVal x As Integer, ByVal y As Integer)
       m_X = x
       m_Y = y
   End Sub

   Public ReadOnly Property X() As Integer
       Get
           Return m_X
       End Get
   End Property

   Public ReadOnly Property Y() As Integer
       Get
           Return m_Y
       End Get
   End Property

End Structure
