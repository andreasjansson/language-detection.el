grass2.height=90
grass2.x=0
grass2.y = display.contentHeight-(grass2.height/1.8)
physics.addBody(grass2, "static", {density=.1, bounce=0.1, friction=.2})
grass2.speed = 6
screenGroup:insert(grass2)  

