DO i = 9, 144
  DO j = 1, 102
  r(i) = ((i*0.20247)**1.17720)
  mu(j) = (j - 2.0)*n + 0.108419524
  IF ( j < 100 .and. i > 16) THEN 
   Psi(i,j) = 0.5*(r(i)**2.0*(1-mu(j)**2.0))  &
  + (30/r(i)**.1)*(1-mu(j))*((r(i)-2.0)*(1-mu(j)) &
  + 2.0*(3-(1+mu(j)*log(1+mu(j))))) &


   + 0.5*(1-mu(j)**2.0)*4*(1/r(i)**1.20)*(1-mu(j)**2.0) * &
  /mu(j)**2.0   + 45*(1*mu(j)**1.2/r(i)**2.0)
  ELSE  
    Psi(i,j) = 0.5*(r(i)**2.0*(1-mu(j)**2.0))


  Psi(i,j) = 20*2*(1+((1-mu(j)**2)/(1-mu(j))**2)*EXP( &
  -2*a**2*mu(j)/(a**2+(1+sqrt(1-a**2.0))**2.0)))**(-1.0)
  END IF
  END DO
  END DO